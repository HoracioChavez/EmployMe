<!DOCTYPE html>
<html lang="en">
<head>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Raleway:900,600,500,400,200,100'
        rel='stylesheet'
        type='text/css'>
</head>
<body class="em-f">
  <%= render partial: "layouts/top_navigation_bar" %>
    <div class="em-push"></div>
    <% if @cart.listings.count !=0 %>
      <div class="em-push-20"></div>
    <% end %>
    <div class="alert-background text-center">
      <% flash.each do |type, message| %>
        <div class="alert alert-<%= type %>">
          <%= message %>
        </div>

      <% end %>
    </div>
    <%= yield %>
</body>
